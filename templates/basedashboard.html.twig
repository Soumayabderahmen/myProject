    <script src="{{asset('doctor/assets/plugins/select2/js/select2.min.js')}}"></script>

    <script src="{{asset('doctor/assets/js/owl.carousel.min.js')}}"></script>

    <script src="{{asset('doctor/assets/plugins/apex/apexcharts.min.js')}}"></script>

    <script src="{{asset('doctor/assets/plugins/apex/chart-data.js')}}"></script>

    <script src="{{asset('doctor/assets/js/script.js')}}"></script>
