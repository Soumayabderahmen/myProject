{% block title %}{% endblock %}

{% block body %}
	<!DOCTYPE html>
	<html
		lang="en">

		<!-- Mirrored from doccure.dreamguystech.com/html/template/login.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 08 Feb 2023 11:42:01 GMT -->
		<head>
			<meta charset="utf-8">
			<title>Dawini</title>
			<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">

			<link href="{{ asset('admin/assets/img/logo_small.png') }}" rel="icon">

			<link rel="stylesheet" href="{{ asset('login/assets/css/bootstrap.min.css') }}">

			<link rel="stylesheet" href="{{ asset('login/assets/plugins/fontawesome/css/fontawesome.min.css') }}">
			<link rel="stylesheet" href="{{ asset('login/assets/plugins/fontawesome/css/all.min.css') }}">

			<link rel="stylesheet" href="{{ asset('login/assets/css/feather.css') }}">

			<link rel="stylesheet" href="{{ asset('login/assets/css/style.css') }}">
		</head>
		<body class="account-page">
			{% if app.session.flashBag.get('error') is not empty %}
				<div class="alert alert-danger">
					{% for message in app.session.flashBag.get('error') %}
						{{ message }}
					{% endfor %}
				</div>
			{% endif %}
			<div class="main-wrapper">
				<div class="content">
					<div class="container-fluid">
						<div class="row">
							<div class="col-md-8 offset-md-2">
								<div class="account-content">
									<div class="row align-items-center justify-content-center">
										<div class="col-md-7 col-lg-6 login-left">
											<img src="{{ asset('login/assets/img/user.webp') }}" class="img-fluid" alt="Doccure Login">
										</div>
										<div class="col-md-12 col-lg-6 login-right">
											<div class="login-header">
												<h3>Login
													<span>Dawini</span>
												</h3>
											</div>
											<form method="post">

												{% if error %}
													<div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
												{% endif %}
												{% if app.user %}
													{# <div class="mb-3">
																														You are logged in as
																														{{ app.user.userIdentifier }},
																														<a href="{{ path('app_logout') }}">Logout</a>
																													</div> #}
												{% endif %}
												<div class="form-group form-focus">

													<input type="email" value="{{ last_username }}" name="email" id="inputEmail" class="form-control" autocomplete="email" required autofocus>
													<label class="focus-label" for="inputEmail">Email</label>
												</div>
												<div class="form-group form-focus">
													<input type="password" name="password" id="inputPassword" class="form-control" autocomplete="current-password" required>
													<label class="focus-label" for="inputPassword">Password</label>
													<input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
												</div>
												<div class="col-12">
													<div class="doctor-submit text-end">
														<div class="text-right">
														<div class="login-header">
												<h6>
													
													<a href="{{ path('forgot') }}">Mot de passe oublié?</a>
												</h6>
											</div>
															
														</div>

													</div>
												</div>


												{# <div class="text-center dont-have">
																										<a href="{{ path('forgot') }}">Mot de passe oublié?</a>
																									</div> #}
																									
												<button class="btn btn-primary w-100 btn-lg login-btn" type="submit">Login</button>
												<div class="login-or">
													<span class="or-line"></span>
													<span class="span-or">or</span>
												</div>

												{# <div class="row form-row social-login">
																										<div class="col-6">
																											<a href="#" class="btn btn-facebook w-100">
																												<i class="fab fa-facebook-f me-1"></i>
																												Login</a>
																										</div>
																										<div class="col-6">
																											<a href="#" class="btn btn-google w-100">
																												<i class="fab fa-google me-1"></i>
																												Login</a>
																										</div>
																									</div> #}
												<div class="text-center dont-have">Vous n'avez pas de compte ?
													<a href="{{ path('app_register') }}">Inscription</a>
												</div>

											</div>
										</div>
									</div>

								</div>
							</div>
						</div>
					</div>


				</div>
				<script data-cfasync="false" src="https://doccure.dreamguystech.com/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
				<script src="{{asset('login/assets/js/jquery-3.6.0.min.js') }}"></script>
				<script src="{{ asset('login/assets/js/bootstrap.bundle.min.js') }}"></script>
				<script src="{{ asset('login/assets/js/feather.min.js') }}"></script>
				<script src="{{ asset('login/assets/js/script.js') }}"></script>
			</body>
		</body>
	</html>
{% endblock %}</body></html>

